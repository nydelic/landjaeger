{
  "routes": [
    {
      "src": "/(?<filepath>.+)\\.(?<ext>mp4)",
      "dest": "https://media.githubusercontent.com/media/nydelic/landjaeger/develop/$filepath.$ext",
      "headers": {
        "Cache-Control": "s-maxage=31536000"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/:path(.mp4)",
      "destination": "https://media.githubusercontent.com/media/nydelic/landjaeger/develop/:path*"
    }
  ]
}
